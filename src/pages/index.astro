						<div class="flex flex-col sm:flex-row gap-2 sm:gap-4">
							<input type="email" name="email" id="email" placeholder="Email" class="border border-white px-3 py-[2px] text-white bg-transparent placeholder-white sm:w-1/2">
							<input type="text" name="tel" id="tel" placeholder="Tel." class="border border-white px-3 py-[2px] text-white bg-transparent placeholder-white sm:w-1/2">
						</div>
